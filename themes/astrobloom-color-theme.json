{
  "name": "AstroBloom",
  "colors": {
    // Editor
    "focusBorder": "#1a1a1a",
    "editor.background": "#222222",
    "editor.foreground": "#fff",
    "editorCursor.foreground": "#dfdfdf",
    "editor.lineHighlightBackground": "#4949492a",
    "editor.selectionHighlightBackground": "#7C9EFF40",
    "editor.selectionBackground": "#746f7757",
    "errorForeground": "#FC644D",

    "editorError.foreground": "#FC644D",
    "editorWarning.foreground": "#FFD66D",
    "editorInfo.foreground": "#89DDFF",
    "editorHint.foreground": "#8FA8FF",

    // Activity Bar
    "activityBar.background": "#191919",
    "activityBar.foreground": "#fff",
    "activityBarBadge.background": "#00ffc8",
    "activityBarBadge.foreground": "#000814",

    // Side Bar
    "sideBar.background": "#191919",
    "sideBar.foreground": "#dfdfdf",
    "sideBarTitle.foreground": "#dfdfdf",

    // Status Bar
    "statusBar.background": "#00ffc8",
    "statusBar.foreground": "#1a1a1a",
    "statusBar.noFolderBackground": "#FFD66D",
    "statusBar.debuggingBackground": "#FF5370",
    "statusBar.debuggingForeground": "#dfdfdf",

    // Tabs
    "tab.activeBackground": "#1a1a1a",
    "tab.activeForeground": "#fff",
    "tab.inactiveBackground": "#222222",
    "tab.inactiveForeground": "#65737E",

    // Title Bar
    "titleBar.activeBackground": "#191919",
    "titleBar.activeForeground": "#dfdfdf",
    "titleBar.inactiveBackground": "#191919",
    "titleBar.inactiveForeground": "#65737E",

    // Buttons
    "button.background": "#89DDFF",
    "button.foreground": "#000",
    "button.hoverBackground": "#6699FF",

    // Minimap
    "minimap.background": "#222222",

    // Editor Gutter
    "editorGutter.background": "#222222",

    // Terminal
    "terminal.background": "#1f1f1f",
    "terminal.foreground": "#fff",

    // Scrollbars
    "scrollbarSlider.background": "#65737E40",
    "scrollbarSlider.hoverBackground": "#65737E80",
    "scrollbarSlider.activeBackground": "#65737EFF",

    // Dropdowns / Lists
    "dropdown.background": "#222222",
    "dropdown.foreground": "#fff",
    "list.activeSelectionBackground": "#383838",
    "list.activeSelectionForeground": "#fff",
    "list.hoverBackground": "#383838",
    "list.hoverForeground": "#fff",

    // Peek / Find
    "peekViewEditor.background": "#1f1f1f",
    "peekViewResult.background": "#1f1f1f",
    "peekViewTitle.background": "#191919",
    "peekViewTitleLabel.foreground": "#dfdfdf",

    // Diff editor
    "diffEditor.insertedTextBackground": "#C3E88D40",
    "diffEditor.removedTextBackground": "#FF537040",

    //
    "editorGutter.modifiedBackground": "#FFE66D",
    "editorGutter.addedBackground": "#7bd88f",
    "editorGutter.deletedBackground": "#FF5370",

    // suggestion modal:
    "editorSuggestWidget.background": "#222222",
    "editorSuggestWidget.foreground": "#fff",
    "editorSuggestWidget.border": "#222222",
    "editorSuggestWidget.highlightForeground": "#89DDFF",
    "editorSuggestWidget.selectedBackground": "#37373D"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#858585",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.control",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable.name", "variable"],
      "settings": {
        "foreground": "#f0f0f0"
      }
    },
    {
      "name": "Types and interfaces",
      "scope": ["entity.name.type", "meta.type"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "keyword.operator.new",
        "variable.language.this",
        "variable.language.this.js",
        "variable.language.this.ts"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "name": "Constants / Arguments / Tag Attributes",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#08CFFF"
      }
    },

    {
      "name": "Template Literals",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.double",
        "string.quoted.triple",
        "string.quoted.single"
      ],
      "settings": {
        "foreground": "#FFE66D"
      }
    },
    {
      "name": "Variable Storage Keywords",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#c74ded"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "name": "HTML punctuation",
      "scope": ["meta.tag"],
      "settings": {
        "foreground": "#8b888f"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "entity.name.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "name": "JSX/HTML Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#89DDFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Terminator for punctuation",
      "scope": ["punctuation.terminator"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "JSX Children Text",
      "scope": ["meta.jsx.children", "string.unquoted", "text.html.basic"],
      "settings": {
        "foreground": "#D5CED9"
      }
    },
    {
      "name": "Braces and Brackets",
      "scope": [
        "punctuation.section.block.begin",
        "punctuation.section.block.end",
        "punctuation.section.brackets.begin",
        "punctuation.section.brackets.end",
        "punctuation.section.group.begin",
        "punctuation.section.group.end",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#08CFFF"
      }
    },
    {
      "name": "Variable Definitions",
      "scope": [
        "variable.other.constant",
        "constant.numeric",
        "constant.numeric.float",
        "constant.numeric.integer",
        "constant.numeric.hex"
      ],
      "settings": {
        "foreground": "#08CFFF"
      }
    },

    {
      "name": "Text Color",
      "scope": [
        "meta.template.expression.js",
        "constant.name.attribute.tag.jade",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.string.begin.markdown"
      ],
      "settings": {
        "foreground": "#D5CED9"
      }
    },
    {
      "name": "Async/Await/Export/Import Keywords",
      "scope": [
        "keyword.control.async",
        "keyword.control.await",
        "keyword.control.import",
        "keyword.control.export",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "name": "Object Keys",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name.json",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#E0F0FF",
        "fontStyle": ""
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.character",
        "constant.other",
        "meta.number",
        "meta.function-call",
        "meta.class",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#00ffc8"
      }
    },
    {
      "name": "HTML/JSX Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "meta.tag.sgml",
        "markup.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "name": "Tag punctuation",
      "scope": [
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#8b888f"
      }
    },
    {
      "name": "Dot Accessor",
      "scope": [
        "punctuation.accessor.js",
        "support.type.object.module.js",
        "support.type.object.exports.js",
        "punctuation.accessor.js"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    }
  ]
}
